    <!-- Question 1: Calcium Carbonate Decomposition -->
    <section ident="yield_1" title="Question 1">
      <item ident="yield_q1" title="Calcium Carbonate Decomposition">
        <itemmetadata>
          <qtimetadata>
            <qtimetadatafield>
              <fieldlabel>question_type</fieldlabel>
              <fieldentry>multiple_choice_question</fieldentry>
            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>1</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html"><![CDATA[
              <p><strong>[Burdge Ch. 11.6 | COR 71.10]</strong></p>
              <p>When 50.0 g CaCO₃ decomposes, 25.2 g CaO is obtained. What is the percent yield?</p>
              <p style="text-align:center; background-color:#f0f0f0; padding:10px; font-family:monospace;">
                CaCO₃(s) → CaO(s) + CO₂(g)
              </p>
            ]]></mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="A">
                <material>
                  <mattext texttype="text/plain">50.4%</mattext>
                </material>
              </response_label>
              <response_label ident="B">
                <material>
                  <mattext texttype="text/plain">75.0%</mattext>
                </material>
              </response_label>
              <response_label ident="C">
                <material>
                  <mattext texttype="text/plain">90.0%</mattext>
                </material>
              </response_label>
              <response_label ident="D">
                <material>
                  <mattext texttype="text/plain">95.0%</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">C</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>
    </section>
    
